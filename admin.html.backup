        }

        :root {
            --bg-dark: #0a0a0f;
            --bg-card: #1a1a2e;
            --bg-input: #16162a;
            --text-primary: #ffffff;
            --text-secondary: #a0a0b8;
            --neon-purple: #9d4edd;
            --neon-pink: #e91e84;
            --success: #10b981;
