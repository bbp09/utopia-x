    <script>
        console.log('üîß Admin page loaded');

        // Load dancers on page load
        document.addEventListener('DOMContentLoaded', () => {
            console.log('üìÑ DOM ready, loading dancers...');
            loadDancers();
        });

        // Tab switching
        function switchTab(tabName) {
            console.log('üîÑ Switching to tab:', tabName);

            // Update tab buttons
            document.querySelectorAll('.tab').forEach(tab => {
                tab.classList.remove('active');
            });
            document.querySelector(`.tab[data-tab="${tabName}"]`).classList.add('active');

            // Update tab content
            document.querySelectorAll('.tab-content').forEach(content => {
                content.classList.remove('active');
            });
            document.getElementById(`tab-${tabName}`).classList.add('active');
        }

        // Add dancer form submission
        document.getElementById('addDancerForm').addEventListener('submit', async (e) => {
            e.preventDefault();
            console.log('‚ûï Adding new dancer...');

            const name = document.getElementById('dancerName').value.trim();
            const genre = document.getElementById('dancerGenre').value.trim();
            let imageUrl = document.getElementById('dancerImage').value.trim();
            const needsReview = document.getElementById('needsReview').checked;
            const isPremium = document.getElementById('isPremium').checked;

            // Validation
            if (!name || !genre) {
                showToast('Ïù¥Î¶ÑÍ≥º Ïû•Î•¥Î•º ÏûÖÎ†•Ìï¥Ï£ºÏÑ∏Ïöî', 'error');
                return;
            }

            // Generate random Unsplash image if not provided
            if (!imageUrl) {
                const randomId = Math.floor(Math.random() * 1000);
                imageUrl = `https://images.unsplash.com/photo-${1500000000000 + randomId}?w=400&h=400&fit=crop`;
                console.log('üé≤ Generated random image:', imageUrl);
            }

            // Determine status
            const status = needsReview ? 'pending' : 'approved';
            console.log('üìã Status:', status, '| Premium:', isPremium);

            try {
                const { data, error } = await window.supabase
                    .from('dancers')
                    .insert([{
                        name: name,
                        genre: genre,
                        image_url: imageUrl,
                        is_premium: isPremium,
                        status: status,
                        rating: 0,
                        bio: `${name} - ${genre} ÎåÑÏÑú`
                    }])
                    .select();

                if (error) {
                    console.error('‚ùå Insert error:', error);
                    showToast('Îì±Î°ù Ïã§Ìå®: ' + error.message, 'error');
                    return;
                }

                console.log('‚úÖ Dancer added:', data);
                const statusText = needsReview ? 'ÏäπÏù∏ ÎåÄÍ∏∞ ÏÉÅÌÉúÎ°ú' : 'ÏäπÏù∏Îêú ÏÉÅÌÉúÎ°ú';
                showToast(`${name} ÎåÑÏÑúÍ∞Ä ${statusText} Îì±Î°ùÎêòÏóàÏäµÎãàÎã§!`, 'success');

                // Reset form
                document.getElementById('addDancerForm').reset();

                // Reload list
                setTimeout(() => loadDancers(), 500);
            } catch (err) {
                console.error('‚ùå Exception:', err);
                showToast('Îì±Î°ù Ï§ë Ïò§Î•òÍ∞Ä Î∞úÏÉùÌñàÏäµÎãàÎã§', 'error');
            }
        });

        // Load dancers from database
        async function loadDancers() {
            console.log('üì• Loading dancers from database...');

            try {
                const { data, error } = await window.supabase
                    .from('dancers')
                    .select('*')
                    .order('created_at', { ascending: false });

                if (error) {
                    console.error('‚ùå Query error:', error);
                    showToast('Îç∞Ïù¥ÌÑ∞ Î°úÎìú Ïã§Ìå®: ' + error.message, 'error');
                    return;
                }

                console.log('‚úÖ Loaded dancers:', data.length);

                // Filter data
                const allDancers = data;
                const pendingDancers = data.filter(d => d.status === 'pending');
                const premiumDancers = data.filter(d => d.is_premium === true);

                // Update counts
                document.getElementById('allCount').textContent = allDancers.length;
                document.getElementById('pendingCount').textContent = pendingDancers.length;
                document.getElementById('premiumCount').textContent = premiumDancers.length;

                // Render tables
                renderAllDancers(allDancers);
                renderPendingDancers(pendingDancers);
                renderPremiumDancers(premiumDancers);
            } catch (err) {
                console.error('‚ùå Exception:', err);
                showToast('Îç∞Ïù¥ÌÑ∞ Î°úÎìú Ï§ë Ïò§Î•òÍ∞Ä Î∞úÏÉùÌñàÏäµÎãàÎã§', 'error');
            }
        }

        // Render all dancers
        function renderAllDancers(dancers) {
            const tbody = document.getElementById('allDancersTableBody');

            if (dancers.length === 0) {
                tbody.innerHTML = `
                    <tr>
                        <td colspan="7" style="text-align: center; padding: 40px; color: var(--text-secondary);">
                            <i class="fas fa-inbox"></i><br>
                            Îì±Î°ùÎêú ÎåÑÏÑúÍ∞Ä ÏóÜÏäµÎãàÎã§
                        </td>
                    </tr>
                `;
                return;
            }

            tbody.innerHTML = dancers.map(dancer => {
                const isPending = dancer.status === 'pending';
                const rowClass = isPending ? 'pending-row' : '';
                
                return `
                    <tr class="${rowClass}">
                        <td>
                            <img src="${dancer.image_url || 'https://via.placeholder.com/60'}" 
                                 alt="${dancer.name}" 
                                 class="dancer-img"
                                 onerror="this.src='https://via.placeholder.com/60'">
                        </td>
                        <td style="font-weight: 600;">${dancer.name}</td>
                        <td>${dancer.genre || '-'}</td>
                        <td>
                            <span class="status-badge ${dancer.status || 'approved'}">
                                ${dancer.status === 'pending' ? '‚è≥ ÏäπÏù∏ ÎåÄÍ∏∞' : '‚úÖ ÏäπÏù∏ ÏôÑÎ£å'}
                            </span>
                        </td>
                        <td>
                            ${dancer.is_premium ? '<span class="premium-badge"><i class="fas fa-star"></i> Premium</span>' : '<span style="color: var(--text-secondary);">-</span>'}
                        </td>
                        <td style="color: var(--text-secondary); font-size: 13px;">
                            ${dancer.created_at ? new Date(dancer.created_at).toLocaleDateString('ko-KR') : '-'}
                        </td>
                        <td>
                            <div class="action-buttons">
                                ${isPending ? `
                                    <button class="btn-approve" onclick="approveDancer('${dancer.id}', '${dancer.name}')">
                                        <i class="fas fa-check"></i> ÏäπÏù∏
                                    </button>
                                ` : `
                                    <button class="btn-toggle-premium" onclick="togglePremium('${dancer.id}', '${dancer.name}', ${dancer.is_premium})">
                                        <i class="fas fa-star"></i> ${dancer.is_premium ? 'Ìï¥Ï†ú' : 'ÏßÄÏ†ï'}
                                    </button>
                                `}
                                <button class="btn-delete" onclick="deleteDancer('${dancer.id}', '${dancer.name}')">
                                    <i class="fas fa-trash"></i> ÏÇ≠Ï†ú
                                </button>
                            </div>
                        </td>
                    </tr>
                `;
            }).join('');
        }

        // Render pending dancers
        function renderPendingDancers(dancers) {
            const tbody = document.getElementById('pendingDancersTableBody');

            if (dancers.length === 0) {
                tbody.innerHTML = `
                    <tr>
                        <td colspan="5" style="text-align: center; padding: 40px; color: var(--text-secondary);">
                            <i class="fas fa-check-circle"></i><br>
                            ÏäπÏù∏ ÎåÄÍ∏∞ Ï§ëÏù∏ ÎåÑÏÑúÍ∞Ä ÏóÜÏäµÎãàÎã§
                        </td>
                    </tr>
                `;
                return;
            }

            tbody.innerHTML = dancers.map(dancer => `
                <tr class="pending-row">
                    <td>
                        <img src="${dancer.image_url || 'https://via.placeholder.com/60'}" 
                             alt="${dancer.name}" 
                             class="dancer-img"
                             onerror="this.src='https://via.placeholder.com/60'">
                    </td>
                    <td style="font-weight: 600;">${dancer.name}</td>
                    <td>${dancer.genre || '-'}</td>
                    <td style="color: var(--text-secondary); font-size: 13px;">
                        ${dancer.created_at ? new Date(dancer.created_at).toLocaleDateString('ko-KR') : '-'}
                    </td>
                    <td>
                        <div class="action-buttons">
                            <button class="btn-approve" onclick="approveDancer('${dancer.id}', '${dancer.name}')">
                                <i class="fas fa-check"></i> ÏäπÏù∏
                            </button>
                            <button class="btn-delete" onclick="deleteDancer('${dancer.id}', '${dancer.name}')">
                                <i class="fas fa-trash"></i> ÏÇ≠Ï†ú
                            </button>
                        </div>
                    </td>
                </tr>
            `).join('');
        }

        // Render premium dancers
        function renderPremiumDancers(dancers) {
            const tbody = document.getElementById('premiumDancersTableBody');

            if (dancers.length === 0) {
                tbody.innerHTML = `
                    <tr>
                        <td colspan="5" style="text-align: center; padding: 40px; color: var(--text-secondary);">
                            <i class="fas fa-star"></i><br>
                            ÌîÑÎ¶¨ÎØ∏ÏóÑ ÎåÑÏÑúÍ∞Ä ÏóÜÏäµÎãàÎã§
                        </td>
                    </tr>
                `;
                return;
            }

            tbody.innerHTML = dancers.map(dancer => `
                <tr>
                    <td>
                        <img src="${dancer.image_url || 'https://via.placeholder.com/60'}" 
                             alt="${dancer.name}" 
                             class="dancer-img"
                             onerror="this.src='https://via.placeholder.com/60'">
                    </td>
                    <td style="font-weight: 600;">${dancer.name}</td>
                    <td>${dancer.genre || '-'}</td>
                    <td style="color: var(--text-secondary); font-size: 13px;">
                        ${dancer.created_at ? new Date(dancer.created_at).toLocaleDateString('ko-KR') : '-'}
                    </td>
                    <td>
                        <div class="action-buttons">
                            <button class="btn-toggle-premium" onclick="togglePremium('${dancer.id}', '${dancer.name}', true)">
                                <i class="fas fa-times"></i> ÌîÑÎ¶¨ÎØ∏ÏóÑ Ìï¥Ï†ú
                            </button>
                            <button class="btn-delete" onclick="deleteDancer('${dancer.id}', '${dancer.name}')">
                                <i class="fas fa-trash"></i> ÏÇ≠Ï†ú
                            </button>
                        </div>
                    </td>
                </tr>
            `).join('');
        }

        // Approve dancer
        async function approveDancer(id, name) {
            console.log('‚úÖ Approving dancer:', id, name);

            try {
                const { error } = await window.supabase
                    .from('dancers')
                    .update({ status: 'approved' })
                    .eq('id', id);

                if (error) {
                    console.error('‚ùå Update error:', error);
                    showToast('ÏäπÏù∏ Ïã§Ìå®: ' + error.message, 'error');
                    return;
                }

                console.log('‚úÖ Dancer approved');
                showToast(`${name} ÎåÑÏÑúÍ∞Ä ÏäπÏù∏ÎêòÏóàÏäµÎãàÎã§!`, 'success');

                // Reload list
                setTimeout(() => loadDancers(), 500);
            } catch (err) {
                console.error('‚ùå Exception:', err);
                showToast('ÏäπÏù∏ Ï§ë Ïò§Î•òÍ∞Ä Î∞úÏÉùÌñàÏäµÎãàÎã§', 'error');
            }
        }

        // Toggle premium status
        async function togglePremium(id, name, currentStatus) {
            const newStatus = !currentStatus;
            const action = newStatus ? 'ÌîÑÎ¶¨ÎØ∏ÏóÑÏúºÎ°ú ÏßÄÏ†ï' : 'ÌîÑÎ¶¨ÎØ∏ÏóÑ Ìï¥Ï†ú';
            console.log('‚≠ê Toggling premium:', id, name, '->', newStatus);

            try {
                const { error } = await window.supabase
                    .from('dancers')
                    .update({ is_premium: newStatus })
                    .eq('id', id);

                if (error) {
                    console.error('‚ùå Update error:', error);
                    showToast(action + ' Ïã§Ìå®: ' + error.message, 'error');
                    return;
                }

                console.log('‚úÖ Premium status toggled');
                showToast(`${name} ÎåÑÏÑúÍ∞Ä ${action}ÎêòÏóàÏäµÎãàÎã§!`, 'success');

                // Reload list
                setTimeout(() => loadDancers(), 500);
            } catch (err) {
                console.error('‚ùå Exception:', err);
                showToast(action + ' Ï§ë Ïò§Î•òÍ∞Ä Î∞úÏÉùÌñàÏäµÎãàÎã§', 'error');
            }
        }

        // Delete dancer
        async function deleteDancer(id, name) {
            if (!confirm(`Ï†ïÎßêÎ°ú "${name}" ÎåÑÏÑúÎ•º ÏÇ≠Ï†úÌïòÏãúÍ≤†ÏäµÎãàÍπå?\n\nÏù¥ ÏûëÏóÖÏùÄ ÎêòÎèåÎ¶¥ Ïàò ÏóÜÏäµÎãàÎã§.`)) {
                return;
            }

            console.log('üóëÔ∏è Deleting dancer:', id, name);

            try {
                const { error } = await window.supabase
                    .from('dancers')
                    .delete()
                    .eq('id', id);

                if (error) {
                    console.error('‚ùå Delete error:', error);
                    showToast('ÏÇ≠Ï†ú Ïã§Ìå®: ' + error.message, 'error');
                    return;
                }

                console.log('‚úÖ Dancer deleted');
                showToast(`${name} ÎåÑÏÑúÍ∞Ä ÏÇ≠Ï†úÎêòÏóàÏäµÎãàÎã§`, 'success');

                // Reload list
                setTimeout(() => loadDancers(), 500);
            } catch (err) {
                console.error('‚ùå Exception:', err);
                showToast('ÏÇ≠Ï†ú Ï§ë Ïò§Î•òÍ∞Ä Î∞úÏÉùÌñàÏäµÎãàÎã§', 'error');
            }
        }

        // Show toast notification
        function showToast(message, type = 'success') {
            const toast = document.getElementById('toast');
            toast.textContent = message;
            toast.className = `toast ${type} show`;

            setTimeout(() => {
                toast.classList.remove('show');
            }, 3000);
        }
    </script>
</body>
