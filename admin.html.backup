        .btn-home {
            background: rgba(255, 255, 255, 0.2);
            border: 2px solid rgba(255, 255, 255, 0.3);
            color: white;
            padding: 12px 24px;
            border-radius: 12px;
            font-size: 14px;
            font-weight: 600;
            cursor: pointer;
            transition: all 0.3s;
            text-decoration: none;
            display: inline-flex;
            align-items: center;
            gap: 8px;
        }

        .btn-home:hover {
            background: rgba(255, 255, 255, 0.3);
            transform: translateY(-2px);
        }

        /* Add Section */
        .add-section {
            background: var(--bg-card);
            padding: 30px;
            border-radius: 20px;
            margin-bottom: 40px;
            border: 2px solid rgba(157, 78, 221, 0.3);
        }

        .add-section h2 {
            font-size: 24px;
            margin-bottom: 20px;
            color: var(--neon-purple);
            display: flex;
            align-items: center;
            gap: 10px;
        }

        .form-grid {
